{
  "name": "postyou/contao-wrapper_tags",
  "type": "contao-bundle",
  "description": "wrapper_tags extension for Contao Open Source CMS",
  "keywords": ["contao", "wrapper", "tags"],
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Maciej Ostrowski",
      "homepage": "http://contao-developer.pl",
      "email": "maciek@zmyslni.pl"
    },
    {
      "name": "Mario Gienapp",
      "email":"mario.gienapp@postyou.de"
    }
  ],
  "require": {
    "contao/core-bundle": "^4.9 || ^5.2",
    "menatwork/contao-multicolumnwizard-bundle": "^3.6",
    "php": ">=8.0"
  },
  "autoload":{
    "psr-4": {
      "Postyou\\ContaoWrapperTags\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Postyou\\ContaoWrapperTags\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "php-http/discovery": true
    }
  }
}
